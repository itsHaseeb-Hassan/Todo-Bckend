{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "15mb",
        "memory": 512,
        "maxDuration": 10 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js",
      "methods":["GET","POST","DELETE","UPDATE","PUT","PATCH","OPTIONS"]
    }
  ]
}
    